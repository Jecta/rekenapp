  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/",  "dest": "/api/index.php" },
    { "src": "/index.php",  "dest": "/api/index.php" },
    { "src": "/eenhedenOmrekenen.php",  "dest": "/api/eenhedenOmrekenen.php" },
    { "src": "/gedeeldDoor.php",  "dest": "/api/gedeeldDoor.php" },
    { "src": "/min.php",  "dest": "/api/min.php" },
    { "src": "/tafels.php",  "dest": "/api/tafels.php" },
    { "src": "/tafelsLeren.php",  "dest": "/api/tafelsLeren.php" },
    { "src": "/tafelsOefenen.php",  "dest": "/api/tafelsOefenen.php" },
    { "src": "/css/style.css", "dest": "/api/css/style.css" },
    { "src": "/assets/js/bereken.js", "dest": "/api/assets/js/bereken.js" },
    { "src": "/assets/js/eenheden.js", "dest": "/api/assets/js/eenheden.js" },
    { "src": "/assets/js/main.js", "dest": "/api/assets/js/main.js" },
    { "src": "/assets/js/tafels.js", "dest": "/api/assets/js/tafels.js" }]
}